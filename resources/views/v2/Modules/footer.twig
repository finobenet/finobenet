<script type="text/javascript">

if(!document.getElementById('ZMRrwOVaKiIQ')) {
    function toggleElement() {
        var parentDiv = document.querySelector(".nav-scroller.navbar-{% if user.theme == 1 or user.gary == 1 %}{{ "dark" }}{% else %}{{ "light" }}{% endif %}.bg-{{ (user.theme == 1 or user.gary == 1 or user.theme == "halloween") ? "dark" : "faded" }}");
        var alert = document.getElementById("dynamicElement");
    
        if (!alert) {
            function generateID(length) {
                var result = '';
                var characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
                var charactersLength = characters.length;
                for (var i = 0; i < length; i++) {
                    result += characters.charAt(Math.floor(Math.random() * charactersLength));
                }
                return result;
            }
            
            parentDiv.insertAdjacentHTML("afterend", '<div id="dynamicElement" class="' + generateID(32) + ' alert alert-danger text-center" style="display: block; color: rgba(255, 255, 255, 0.9); font-size: 1.25rem; word-break: break-all;">Hey! We rely on ads to survive and upgrade our servers. Perhaps whitelist us?</div>');
        }
    }
    
    setInterval(toggleElement, 10);
}
</script>
{% endif %}