	  <div class="my-4">
        <ul class="pagination shadow-sm">
			<li class="page-item{{ (data.videos.pages.info.previous_page == data.videos.pages.info.start_page) ? " disabled" : "" }}">
			  <a class="page-link" id="previous-page" href="/videos?page={{ data.videos.pages.info.previous_page }}" rel="previous" aria-label="Previous <<" {{ (data.videos.pages.info.current_page <= 1) ? "style='pointer-events: none;'" : "" }}>Â«</a>
			</li>
			{% if data.videos.pages.info.start_page > 1 %}
			  <li class="page-item">
				<a class="page-link" href="/videos?page=1">1</a>
			  </li>
			  {% if data.videos.pages.info.start_page > 2 %}
				<li class="page-item">
				  <a class="page-link" href="/videos?page=2">2</a>
				</li>
			  {% endif %}
			  <li class="page-item"><span class="page-link">...</span></li>
			{% endif %}
			{% for page in data.videos.pages.data %}
