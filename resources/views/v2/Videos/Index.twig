					{% for video in videosBatch %}
						<div class="col-lg-2 mb-4">
							<a href="/video/{{ video.id }}" alt="{{ video.title }}" class="game-card h-100 video-card">
								<span class="thumbnail"><img src="/video/thumb/{{ video.id }}" alt="{{ video.title }}" class="card-img-top"></span>
								<span class="data py-2 px-2 d-flex flex-column">
									<span class="catalog-no-overflow-plz">{{ video.title }}</span>
									<span class="catalog-no-overflow-plz text-muted">{{ video.author }}</span>
								</span>
							</a>
						</div>
					{% endfor %}
				</div>
			</div>
		{% endfor %}
